{
  "AllowedHosts": "*",
  "ApplicationLogging": {
    "Extent": "ActivityOnly",
    "OnlyErrors": false
  },
  "Caching": {
    "ActorLifetime": "01:00:00"
  },
  "Cookies": {
    "RefreshToken": {
      "HttpOnly": true,
      "MaxAge": "7.00:00:00",
      "SameSite": "Strict",
      "Secure": true
    },
    "Session": {
      "SameSite": "Strict"
    }
  },
  "Cors": {
    "AllowAnyOrigin": false,
    "AllowedOrigins": [],
    "AllowAnyMethod": false,
    "AllowedMethods": [],
    "AllowAnyHeader": false,
    "AllowedHeaders": [],
    "AllowCredentials": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MongoDB": {
    "DatabaseName": "SkillCraftTools"
  },
  "OpenAuthentication": {
    "AccessToken": {
      "Lifetime": 300,
      "TokenType": "at+jwt"
    }
  },
  "Portal": {
    "Realm": "skillcraft-tools"
  }
}
